<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vibhu.pocketmoney">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayActivity" />
        <activity android:name=".PersonalProfileDetails" />
        <activity android:name=".ProfessionalProfileDetails" />
        <activity android:name=".CurrentResidentialAddress" />
        <activity android:name=".ProfilePicture" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".LoanCalculator" />
        <activity android:name=".PhoneNumberEntry" />
        <activity android:name=".ViewProfile" />
        <activity android:name=".AutoPaymentStatus" />
        <activity android:name=".ChooseDocumentMethod" />
        <activity android:name=".InsertDocument" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".PermissionsActivity" />
        <activity android:name=".FaqActivity" />
        <activity android:name=".CashActivity" />
        <activity android:name=".AllLoansTaken" />
        <activity android:name=".ReferEarn" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".AboutUsActivity"></activity>
    </application>

</manifest>